@model IEnumerable<RaportareAjustajV2.PellatriceLandgrafModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<h3>Salut @ViewBag.UserName !</h3>
<p>
    <a asp-action="Create">Create New</a>
</p>
@if (ViewBag.IsAdmin == "True")
{
    <p>
        <a asp-action="ExportToExcelAsync">Exporta in excel</a>
    </p>
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataIntroducere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiametruIntrare)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiametruIesire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Calitate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sarja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eticheta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NrBare)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lungime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Masa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataIntroducere)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiametruIntrare)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiametruIesire)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Calitate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sarja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eticheta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NrBare)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lungime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Masa)
            </td>
            @if (ViewBag.IsAdmin == "True")
            {
                <td>
                    <a asp-action="Edit" asp-route-id="@item.PellatriceLandgrafModelId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.PellatriceLandgrafModelId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.PellatriceLandgrafModelId">Delete</a>
                </td>
            }
        </tr>
        }
    </tbody>
</table>
